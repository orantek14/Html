  <body>
    <script>
      let a = 5;
      switch (a) {
        case 1:
          console.log("przelaczam do dzialu obslugi reklamacji");
          break;
        case 2:
        case 5:
        case 8:
          console.log("Hakuje...");
          break;
        case 3:
          alert("haslo: okon");
        case 4:
        case 6:
        case 7:
        case 9:
          dokmork();
          break;
        default:
          break;
      }

      function dokmork() {
        console.log("provide magic");
      }
      //wole case 
      if (a === 1) {
        console.log("przelaczam do dzialu obslugi reklamacji");
      } else if (a === 2 || a === 5 || a === 8) {
        console.log("Hakuje...");
      } else if (a === 3) {
        alert("haslo: okon");
      } else if (a === 4 || a === 6 || a === 7 || a === 9) {
        dokmork();
      } else {
        // domy≈õlna sytuacja (odpowiednik default)
      }
    </script>
  </body>
